<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maite.semana4">
    <uses-permission android:name="android.permission.INTERNET">

    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".menus.ActivityContacto"></activity>
        <activity android:name=".menus.ActivityAbout" />
        <activity android:name=".menus.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".menus.MisContactos"
            android:parentActivityName=".menus.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".menus.MainActivity" />
        </activity>
    </application>

</manifest>